{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --project mcquiz-global --only functions",
    "deploy:development": "firebase deploy --only functions --project=\"mcquiz---development\"",
    "deploy:preview": "firebase deploy --only functions --project=\"mcquiz---preview\"",
    "deploy:production": "firebase deploy --only functions --project=\"mcquiz-ab4a3\"",
    "deploy:all": "npm run deploy:development && npm run deploy:preview && npm run deploy:production",
    "logs": "firebase functions:log",
    "dummy": ".exec/start-up.sh",
    "dev": "npm run build && firebase emulators:start --import .db-dummy/db --project mcquiz-global --export-on-exit",
    "export": "firebase emulators:export .db-dummy/db --project mcquiz---development"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "dependencies": {
    "@types/lodash": "^4.14.195",
    "firebase-admin": "^11.10.1",
    "firebase-functions": "^4.4.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint": "^8.45.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.27.5",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^5.1.6"
  },
  "private": true
}
